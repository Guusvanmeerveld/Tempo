{
	"name": "tempo",
	"version": "0.1.0",
	"description": "A simple Discord music bot that can play Soundcloud, Youtube or Spotify tracks.",
	"main": "dist/index.js",
	"homepage": "https://tempo.g-vm.nl",
	"repository": "github:guusvanmeerveld/tempo",
	"markdown": "github",
	"scripts": {
		"pretify": "prettier --write .",
		"lint": "eslint src",
		"dev": "ts-node-dev -r tsconfig-paths/register src/bot/start.ts",
		"dev-full": "ts-node-dev src",
		"test-build": "tsc --noEmit",
		"build": "tsc",
		"start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register . --production"
	},
	"author": "Guus van Meerveld",
	"license": "MIT",
	"dependencies": {
		"@discordjs/opus": "^0.5.3",
		"axios": "^0.21.1",
		"bufferutil": "^4.0.3",
		"chalk": "^4.1.1",
		"cheerio": "^1.0.0-rc.10",
		"discord.js-light": "^3.5.11",
		"dotenv": "^10.0.0",
		"ffmpeg-static": "^4.3.0",
		"humanize-duration": "^3.27.0",
		"libsodium-wrappers": "^0.7.9",
		"lodash": "^4.17.21",
		"long": "^4.0.0",
		"m3u8stream": "^0.8.4",
		"pg": "^8.6.0",
		"utf-8-validate": "^5.0.5",
		"ws": "^7.4.6",
		"ytdl-core": "^4.8.2",
		"ytsr": "^3.5.0"
	},
	"devDependencies": {
		"@types/humanize-duration": "^3.25.0",
		"@types/lodash": "^4.14.170",
		"@types/long": "^4.0.1",
		"@types/node": "^15.12.2",
		"@types/pg": "^8.6.0",
		"@types/ws": "^7.4.4",
		"@typescript-eslint/eslint-plugin": "^4.28.1",
		"@typescript-eslint/parser": "^4.26.1",
		"eslint": "^7.28.0",
		"eslint-plugin-prettier": "^3.4.0",
		"prettier": "^2.3.1",
		"ts-node-dev": "^1.1.6",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.3.2"
	}
}
